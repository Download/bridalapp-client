{
  "name": "bridalapp-client",
  "version": "0.10.0",
  "description": "BridalApp Client",
  "repository": {
    "type": "git",
    "url": "git://github.com/download/bridalapp-client.git"
  },
  "author": {
    "name": "Stijn de Witt",
    "email": "StijnDeWitt@hotmail.com",
    "url": "http://StijnDeWitt.com"
  },
  "contributors": [],
  "license": "CC-BY-4.0",
  "licenseUrl": "https://creativecommons.org/licenses/by/4.0/",
  "scripts": {
    "build-js-old": "browserify -t [babelify] src/index.jsx > dist/bundle.js",
    "build-js": "browserify src/index.jsx > dist/bundle.js",
    "build": "npm run build-js"
  },
  "browser": {
    "react": "react",
    "immutable": "immutable"
  },
  "browserify": {
    "basedir": "src",
    "extensions": [
      ".jsx"
    ],
    "entry": [
      "src/index.jsx"
    ],
    "transform": [
      "babelify",
      "browserify-shim"
    ]
  },
  "browserify-shim": {
    "immutable": "global:Immutable",
    "react": "global:React",
    "react-dom": {
      "exports": "global:ReactDOM",
      "depends": [
        "react:React"
      ]
    },
    "morearty": {
      "exports": "Morearty",
      "depends": [
        "react:React",
        "immutable:Immutable"
      ]
    }
  },
  "disabled-devDependencies": {
    "babel-preset-es2015": "^6.1.2",
    "babel-preset-react": "^6.1.2",
    "babelify": "^7.2.0"
  },
  "devDependencies": {
    "babelify": "^7.3.0",
    "browserify": "^13.1.1",
    "browserify-shim": "^3.8.11",
    "gulp": "^3.9.0",
    "gulp-notify": "^2.2.0",
    "react-tap-event-plugin": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.6.0"
  },
  "dependencies": {
    "history": "^4.4.0",
    "immutable": "^3.7.5",
    "material-ui": "^0.16.2",
    "morearty": "^0.7.25",
    "picolog": "^1.0.4",
    "react-dom": "^15.4.0",
    "react-mixin": "^3.0.3",
    "react-router": "^3.0.0",
    "webdb": "^0.5.0",
    "ws.suid": "^0.10.1"
  }
}
